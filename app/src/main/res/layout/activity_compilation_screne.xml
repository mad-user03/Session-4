<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/box_inset_layout_padding"
    tools:context=".CompilationScrene"
    android:background="#332973"
    tools:deviceIds="wear">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foregroundGravity="center"
                android:src="@drawable/im2" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/imageView7"
                android:foregroundGravity="center"
                android:src="@drawable/im4" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_below="@id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foregroundGravity="center"
                android:src="@drawable/im3" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="40dp"
                android:layout_height="36dp"
                android:layout_below="@id/imageView5"
                android:layout_marginTop="-45dp"
                android:background="@drawable/colorbutton"
                android:src="@drawable/likebutton" />

            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="59dp"
                android:layout_height="50dp"
                android:layout_below="@id/imageView5"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="-56dp"
                android:background="#00000000"
                android:src="@drawable/ic_baseline_restore_from_trash_24" />
            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="40dp"
                android:layout_height="36dp"
                android:layout_below="@id/imageView6"
                android:layout_marginTop="-61dp"
                android:background="@drawable/colorbutton"
                android:src="@drawable/likebutton" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="57dp"
                android:layout_height="54dp"
                android:layout_below="@id/imageView6"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="-61dp"
                android:background="#00000000"
                android:src="@drawable/ic_baseline_restore_from_trash_24" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="40dp"
                android:layout_height="36dp"
                android:layout_below="@id/imageView7"
                android:layout_marginTop="-45dp"
                android:background="@drawable/colorbutton"
                android:src="@drawable/likebutton" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="57dp"
                android:layout_height="54dp"
                android:layout_below="@id/imageView7"
                android:layout_marginLeft="170dp"
                android:layout_marginTop="-45dp"
                android:background="#00000000"
                android:src="@drawable/ic_baseline_restore_from_trash_24" />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>